<div id="clearance" class="sign-up">
  <h2 class="title"><%= t(".title") %></h2>

  <%= form_for @user do |form| %>
    <%= render partial: '/users/form', object: form %>

    <div class="field">
      <%= form.submit %>
    </div>

    <div class="field">
      <%= link_to t(".sign_in"), sign_in_path %>
    </div>
  <% end %>
</div>
